# Track large data and persistence with Git LFS
data/** filter=lfs diff=lfs merge=lfs -text
data/* filter=lfs diff=lfs merge=lfs -text
data/persist/** filter=lfs diff=lfs merge=lfs -text
*.npy filter=lfs diff=lfs merge=lfs -text
*.sqlite3 filter=lfs diff=lfs merge=lfs -text
persist/** filter=lfs diff=lfs merge=lfs -text
data/processed/persist/** filter=lfs diff=lfs merge=lfs -text
data/processed/*.npy filter=lfs diff=lfs merge=lfs -text
data/processed/*.parquet filter=lfs diff=lfs merge=lfs -text
